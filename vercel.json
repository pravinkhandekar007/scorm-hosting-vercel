{
  "headers": [
    {
      "source": "/scorm-content/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "fullscreen=(self)"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/get-course",
      "destination": "https://mfooewemvofleynanjkb.supabase.co/functions/v1/get-course"
    },
    {
      "source": "/api/get-course-by-public-id",
      "destination": "https://mfooewemvofleynanjkb.supabase.co/functions/v1/get-course-by-public-id"
    },
    {
      "source": "/scorm-content/:path*",
      "destination": "/api/serve-scorm?path=:path*"
    }
  ]
}
